{
	"servers": {
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		"deepwiki": {
			"type": "http",
			"url": "https://mcp.deepwiki.com/sse",
		},
		"markitdown": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"markitdown-mcp"
			],
		},
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
		}
	},
	"inputs": [
		{
			"id": "memory_file_path",
			"type": "promptString",
			"description": "Path to the memory storage file",
			"password": false
		}
	]
}